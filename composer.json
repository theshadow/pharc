{
  "name": "theshadow/pharc",
  "description": "pharc is a tool which can compile your PHP project into a phar",
  "minimum-stability": "dev",
  "homepage": "https://getcomposer.org/",
  "type": "library",
  "keywords": ["package", "phar", "compile"],
  "authors": [
    {
      "name": "Xander Guzman",
      "email": "xander.guzman@xanderguzman.com"
    }
  ],
  "support": {
    "irc": "irc://irc.freenode.org/pharc",
    "issues": "https://github.com/theshadow/pharc/issues"
  },
  "autoload": {
    "psr-4": {
      "Pharc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pharc\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5",
    "symfony/console": "^2.5",
    "symfony/finder": "^2.2",
    "symfony/process": "^2.1",
    "seld/phar-utils": "^1.0",
    "seld/cli-prompt": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.11.0",
    "phpunit/phpunit": "^4.6.7"
  },
  "bin": ["bin/pharc"],
  "config": {
    "platform": {
      "php": "5.5.22"
    }
  }
}